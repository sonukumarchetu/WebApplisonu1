@{
}
<a asp-action="create"asp-controller="Custmore">create</a>
<table class="table table-bordered">
    <tr>
        <th>
            Id
        </th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>Amount</th>
        <th>Country</th>
        <th>dob</th>
        <th>Address</th>
        <th>Action</th>

    </tr>
    @foreach(var cc in Model)
            {
                <tr>
                    <td>@cc.id</td>
                    <td>@cc.firstname</td>
                    <td>@cc.lastname</td>
                    <td>@cc.email</td>
                    <td>@cc.gender</td>
                    <td>@cc.amount</td>
                    <td>@cc.country</td>
                    <td>@cc.Dob</td>
                    <td>@cc.address</td>
                    <td>
                        <a asp-action="Update" oneclick="return window.confirm('Are you sure to Edit custmore ?')"
                           asp-controller="Custmore" asp-route-id="@cc.id">Edit</a>

                        <a asp-action="Delete" oneclick="return window.confirm('Are you sure to Delete custmore ?')"
                           asp-controller="Custmore" asp-route-id="@cc.id">Delete</a>
                    </td>
                </tr>
            }
</table>