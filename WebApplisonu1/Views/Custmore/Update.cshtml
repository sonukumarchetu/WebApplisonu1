@model WebApplisonu1.Models.Custmore
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<div class="row">
    <div class="col-6 offset-3">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">Create Custmore</h4>
                <div class="card-body">
                    <form asp-action="Update" asp-controller="Custmore"
                          method="post">
                        <input type="hidden" asp-for="id" />
                        <div class="form-group">
                            <table>firstName</table>
                            <input type="text" asp-for="firstname" class="form-control" />
                        </div>
                        <div class="form-group">
                            <table>lastName</table>
                            <input type="text" asp-for="lastname" class="form-control" />
                        </div>
                        <div class="form-group">
                            <table>Email</table>
                            <input type="text" asp-for="email" class="form-control" />
                        </div>
                        <div class="form-group">
                            <table>Gender</table>
                            <select class="form-control" asp-for="gender">
                                <option value="male">male</option>
                                <option value="female">female</option>
                                <option value="transgender">transgender</option>
                            </select>
                        </div>
                        <div class="form-group">
                            <button type="submit" class="btn btn-success">Updates</button>

                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
